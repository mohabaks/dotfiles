#!/bin/sh
. $HOME/.local/bin/color-parser
BLACK="#373B41"
COLOR_DEFAULT_FG="$FG"
COLOR_DEFAULT_BG="$BG"
COLOR_MONITOR_FG="$GREEN"
COLOR_MONITOR_BG="$BG"
COLOR_FOCUSED_MONITOR_FG="$BGREEN"
COLOR_FOCUSED_MONITOR_BG="$BG"
COLOR_FREE_FG="$BBLACK"
COLOR_FREE_BG="$BG"
COLOR_FOCUSED_FREE_FG="$BBLACK"
COLOR_FOCUSED_FREE_BG="$BLACK"
COLOR_OCCUPIED_FG="$FG"
COLOR_OCCUPIED_BG="$BG"
COLOR_FOCUSED_OCCUPIED_FG="$BWHITE"
COLOR_FOCUSED_OCCUPIED_BG="$BLACK"
COLOR_URGENT_FG="$CYAN"
COLOR_URGENT_BG="$BG"
COLOR_FOCUSED_URGENT_FG="$BWHITE"
COLOR_FOCUSED_URGENT_BG="$BLUE"
COLOR_STATE_FG="#89b09c"
COLOR_STATE_BG="$BG"
COLOR_TITLE_FG="#a8a2c0"
COLOR_TITLE_BG="$BG"
COLOR_SYS_FG="#b1a57d"
COLOR_SYS_BG="$BG"
